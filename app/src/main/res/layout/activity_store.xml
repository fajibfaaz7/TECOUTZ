<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.f7.outsiderz.tecoutz.storeActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TecStore"
        android:layout_centerHorizontal="true"
        android:textSize="30dp"
        android:textStyle="bold"
        android:paddingTop="20dp"
        android:textColor="#000000"
        android:id="@+id/tecstr"
        />
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:src="@drawable/storeim"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:paddingTop="50dp"
        android:id="@+id/imageView9"
        />


    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/storebook"
        android:layout_marginTop="34dp"
        android:id="@+id/books"
        android:layout_marginRight="41dp"
        android:layout_marginEnd="41dp"
        android:layout_below="@+id/imageView9"
        android:layout_toLeftOf="@+id/tecstr"
        android:layout_toStartOf="@+id/tecstr" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Books"
        android:layout_marginTop="6dp"
        android:layout_below="@+id/books"
        android:layout_alignRight="@+id/books"
        android:layout_alignEnd="@+id/books"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp"
        android:id="@+id/textView4" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/sports"
        android:id="@+id/imageView11"
        android:layout_alignTop="@+id/books"
        android:layout_toRightOf="@+id/textView4"
        android:layout_toEndOf="@+id/textView4"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sports"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        android:layout_alignBaseline="@+id/textView4"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignRight="@+id/imageView11"
        android:layout_alignEnd="@+id/imageView11"
        android:id="@+id/textView5" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/instru"
        android:id="@+id/instru"
        android:layout_alignTop="@+id/imageView11"
        android:layout_alignLeft="@+id/instrutxt"
        android:layout_alignStart="@+id/instrutxt"
        android:layout_marginLeft="6dp"
        android:layout_marginStart="6dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Instruments"
        android:id="@+id/instrutxt"
        android:layout_marginLeft="52dp"
        android:layout_marginStart="52dp"
        android:layout_alignBaseline="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_toRightOf="@+id/textView5"
        android:layout_toEndOf="@+id/textView5" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/uniform"
        android:id="@+id/uniform"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_alignTop="@+id/instru"
        android:layout_toRightOf="@+id/instrutxt"
        android:layout_toEndOf="@+id/instrutxt" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Uniform"
        android:layout_alignBottom="@+id/instrutxt"
        android:layout_alignRight="@+id/uniform"
        android:layout_alignEnd="@+id/uniform"
        android:layout_alignTop="@+id/instrutxt" />

</RelativeLayout>
